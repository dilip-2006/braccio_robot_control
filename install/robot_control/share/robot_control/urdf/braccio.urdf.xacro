<?xml version="1.0"?>
<robot name="braccio" xmlns:xacro="http://www.ros.org/wiki/xacro">

  <!-- ===== ros2_control hardware interface ===== -->
  <ros2_control name="GazeboSystem" type="system">
    <hardware>
      <plugin>gz_ros2_control/GazeboSystem</plugin>
    </hardware>
    <joint name="base_joint">
      <command_interface name="effort"/>
      <state_interface name="position"/>
      <state_interface name="velocity"/>
      <state_interface name="effort"/>
    </joint>
    <joint name="shoulder_joint">
      <command_interface name="effort"/>
      <state_interface name="position"/>
      <state_interface name="velocity"/>
      <state_interface name="effort"/>
    </joint>
    <joint name="elbow_joint">
      <command_interface name="effort"/>
      <state_interface name="position"/>
      <state_interface name="velocity"/>
      <state_interface name="effort"/>
    </joint>
    <joint name="wrist_pitch_joint">
      <command_interface name="effort"/>
      <state_interface name="position"/>
      <state_interface name="velocity"/>
      <state_interface name="effort"/>
    </joint>
    <joint name="wrist_roll_joint">
      <command_interface name="effort"/>
      <state_interface name="position"/>
      <state_interface name="velocity"/>
      <state_interface name="effort"/>
    </joint>
    <joint name="gripper_joint">
      <command_interface name="effort"/>
      <state_interface name="position"/>
      <state_interface name="velocity"/>
      <state_interface name="effort"/>
    </joint>
    <joint name="sub_gripper_joint">
      <command_interface name="effort"/>
      <state_interface name="position"/>
      <state_interface name="velocity"/>
      <state_interface name="effort"/>
    </joint>
  </ros2_control>

  <!-- ===== Gazebo ros2_control plugin ===== -->
  <gazebo>
    <plugin filename="libgz_ros2_control-system.so" name="gz_ros2_control::GazeboSimROS2ControlPlugin">
      <parameters>$(find robot_control)/config/ros2_controllers.yaml</parameters>
    </plugin>
  </gazebo>

  <!-- ===== Materials ===== -->
  <material name="orange">
    <color rgba="0.57 0.17 0.0 1"/>
  </material>
  <material name="white">
    <color rgba="0.8 0.8 0.8 1.0"/>
  </material>
  <material name="black">
    <color rgba="0 0 0 0.50"/>
  </material>

  <!-- ===== World / Fixed base ===== -->
  <link name="world"/>
  <joint name="world_joint" type="fixed">
    <parent link="world"/>
    <child link="base_link"/>
    <origin rpy="0.0 0.0 0.0" xyz="0.0 0.0 0.0"/>
  </joint>

  <!-- ===== base_link ===== -->
  <link name="base_link">
    <visual>
      <geometry>
        <cylinder length="0.01" radius=".053"/>
      </geometry>
      <material name="black"/>
      <origin rpy="0 0 0" xyz="0 0 0"/>
    </visual>
    <inertial>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <mass value="2"/>
      <inertia ixx="0.015" ixy="0" ixz="0" iyy="0.00666666666667" iyz="0" izz="0.0216666666667"/>
    </inertial>
    <collision>
      <geometry>
        <cylinder length="0.01" radius=".053"/>
      </geometry>
      <origin rpy="0 0 0" xyz="0 0 0"/>
    </collision>
  </link>

  <!-- ===== link_1 (base rotation) ===== -->
  <link name="link_1">
    <visual>
      <geometry>
        <mesh filename="package://robot_control/stl/braccio_base.stl" scale="0.001 0.001 0.001"/>
      </geometry>
      <material name="orange"/>
      <origin rpy="0 0 3.1416" xyz="0 0.004 0"/>
    </visual>
    <collision>
      <geometry>
        <mesh filename="package://robot_control/stl/braccio_base.stl" scale="0.001 0.001 0.001"/>
      </geometry>
      <origin rpy="0 0 3.1416" xyz="0 0.004 0"/>
    </collision>
    <inertial>
      <origin rpy="0 0 3.1416" xyz="0 0.004 0"/>
      <mass value="2"/>
      <inertia ixx="0.015" ixy="0" ixz="0" iyy="0.00666666666667" iyz="0" izz="0.0216666666667"/>
    </inertial>
  </link>

  <!-- ===== link_2 (shoulder) ===== -->
  <link name="link_2">
    <visual>
      <geometry>
        <mesh filename="package://robot_control/stl/braccio_shoulder.stl" scale="0.001 0.001 0.001"/>
      </geometry>
      <material name="orange"/>
      <origin rpy="0 0 0" xyz="-0.0045 0.0055 -0.026"/>
    </visual>
    <collision>
      <geometry>
        <mesh filename="package://robot_control/stl/braccio_shoulder.stl" scale="0.001 0.001 0.001"/>
      </geometry>
      <origin rpy="0 0 0" xyz="-0.0045 0.0055 -0.026"/>
    </collision>
    <inertial>
      <origin rpy="0 0 0" xyz="-0.0045 0.0055 -0.026"/>
      <mass value="0.1"/>
      <inertia ixx="0.000348958333333" ixy="0" ixz="0" iyy="0.000348958333333" iyz="0" izz="3.125e-05"/>
    </inertial>
  </link>

  <!-- ===== link_3 (elbow) ===== -->
  <link name="link_3">
    <visual>
      <geometry>
        <mesh filename="package://robot_control/stl/braccio_elbow.stl" scale="0.001 0.001 0.001"/>
      </geometry>
      <material name="orange"/>
      <origin rpy="0 0 0" xyz="-0.0045 0.005 -0.025"/>
    </visual>
    <collision>
      <geometry>
        <mesh filename="package://robot_control/stl/braccio_elbow.stl" scale="0.001 0.001 0.001"/>
      </geometry>
      <origin rpy="0 0 0" xyz="-0.0045 0.005 -0.025"/>
    </collision>
    <inertial>
      <origin rpy="0 0 0" xyz="-0.0045 0.005 -0.025"/>
      <mass value="0.1"/>
      <inertia ixx="0.000348958333333" ixy="0" ixz="0" iyy="0.000348958333333" iyz="0" izz="3.125e-05"/>
    </inertial>
  </link>

  <!-- ===== link_4 (wrist pitch) ===== -->
  <link name="link_4">
    <visual>
      <geometry>
        <mesh filename="package://robot_control/stl/braccio_wrist_pitch.stl" scale="0.001 0.001 0.001"/>
      </geometry>
      <material name="orange"/>
      <origin rpy="0 0 0" xyz="0.003 -0.0004 -0.024"/>
    </visual>
    <collision>
      <geometry>
        <mesh filename="package://robot_control/stl/braccio_wrist_pitch.stl" scale="0.001 0.001 0.001"/>
      </geometry>
      <origin rpy="0 0 0" xyz="0.003 -0.0004 -0.024"/>
    </collision>
    <inertial>
      <origin rpy="0 0 0" xyz="0.003 -0.0004 -0.024"/>
      <mass value="0.1"/>
      <inertia ixx="0.000348958333333" ixy="0" ixz="0" iyy="0.000348958333333" iyz="0" izz="3.125e-05"/>
    </inertial>
  </link>

  <!-- ===== link_5 (wrist roll) ===== -->
  <link name="link_5">
    <visual>
      <geometry>
        <mesh filename="package://robot_control/stl/braccio_wrist_roll.stl" scale="0.001 0.001 0.001"/>
      </geometry>
      <material name="white"/>
      <origin rpy="0 0 0" xyz="0.006 0 0.0"/>
    </visual>
    <collision>
      <geometry>
        <mesh filename="package://robot_control/stl/braccio_wrist_roll.stl" scale="0.001 0.001 0.001"/>
      </geometry>
      <origin rpy="0 0 0" xyz="0.006 0 0.0"/>
    </collision>
    <inertial>
      <origin rpy="0 0 0" xyz="0.006 0 0.0"/>
      <mass value="0.1"/>
      <inertia ixx="0.000348958333333" ixy="0" ixz="0" iyy="0.000348958333333" iyz="0" izz="3.125e-05"/>
    </inertial>
  </link>

  <!-- ===== left_gripper_link ===== -->
  <link name="left_gripper_link">
    <visual>
      <geometry>
        <mesh filename="package://robot_control/stl/braccio_left_gripper.stl" scale="0.001 0.001 0.001"/>
      </geometry>
      <material name="white"/>
      <origin rpy="0 1.5708 0" xyz="0 -0.012 0"/>
    </visual>
    <inertial>
      <mass value="0.1"/>
      <inertia ixx="0.000348958333333" ixy="0" ixz="0" iyy="0.000348958333333" iyz="0" izz="3.125e-05"/>
    </inertial>
    <collision>
      <origin rpy="0 1.5708 0" xyz="0 -0.012 0"/>
      <geometry>
        <mesh filename="package://robot_control/stl/braccio_left_gripper.stl" scale="0.001 0.001 0.001"/>
      </geometry>
    </collision>
  </link>

  <!-- ===== right_gripper_link ===== -->
  <link name="right_gripper_link">
    <visual>
      <geometry>
        <mesh filename="package://robot_control/stl/braccio_right_gripper.stl" scale="0.001 0.001 0.001"/>
      </geometry>
      <material name="white"/>
      <origin rpy="0 1.5708 0" xyz="0 -0.012 0.010"/>
    </visual>
    <inertial>
      <mass value="0.1"/>
      <inertia ixx="0.000348958333333" ixy="0" ixz="0" iyy="0.000348958333333" iyz="0" izz="3.125e-05"/>
    </inertial>
    <collision>
      <origin rpy="0 1.5708 0" xyz="0 -0.012 0.010"/>
      <geometry>
        <mesh filename="package://robot_control/stl/braccio_right_gripper.stl" scale="0.001 0.001 0.001"/>
      </geometry>
    </collision>
  </link>

  <!-- ===== Joints ===== -->
  <joint name="base_joint" type="revolute">
    <axis xyz="0 0 -1"/>
    <limit effort="1.0" lower="0.0" upper="3.1416" velocity="1.0"/>
    <origin rpy="0 0 0" xyz="0 0 0"/>
    <parent link="base_link"/>
    <child link="link_1"/>
    <dynamics damping="0.1" friction="0.001"/>
  </joint>

  <joint name="shoulder_joint" type="revolute">
    <axis xyz="1 0 0"/>
    <limit effort="1.0" lower="0.2618" upper="2.8798" velocity="1.0"/>
    <origin rpy="-1.5708 0 0" xyz="0 -.002 0.072"/>
    <parent link="link_1"/>
    <child link="link_2"/>
    <dynamics damping="0.1" friction="0.001"/>
  </joint>

  <joint name="elbow_joint" type="revolute">
    <axis xyz="1 0 0"/>
    <limit effort="1.0" lower="0" upper="3.1416" velocity="1.0"/>
    <origin rpy="-1.5708 0 0" xyz="0 0 0.125"/>
    <parent link="link_2"/>
    <child link="link_3"/>
    <dynamics damping="0.1" friction="0.001"/>
  </joint>

  <joint name="wrist_pitch_joint" type="revolute">
    <axis xyz="1 0 0"/>
    <limit effort="1.0" lower="0" upper="3.1416" velocity="1.0"/>
    <origin rpy="-1.5708 0 0" xyz="0 0 0.125"/>
    <parent link="link_3"/>
    <child link="link_4"/>
    <dynamics damping="0.1" friction="0.001"/>
  </joint>

  <joint name="wrist_roll_joint" type="revolute">
    <axis xyz="0 0 -1"/>
    <limit effort="1.0" lower="0.0" upper="3.1416" velocity="1.0"/>
    <origin rpy="0 0 1.5708" xyz="0 0.0 0.06"/>
    <parent link="link_4"/>
    <child link="link_5"/>
    <dynamics damping="0.1" friction="0.001"/>
  </joint>

  <joint name="gripper_joint" type="revolute">
    <axis xyz="0 -1 0"/>
    <limit effort="1.0" lower="0.1750" upper="1.2741" velocity="1.0"/>
    <origin rpy="0 -0.2967 0" xyz="0.010 0 0.03"/>
    <parent link="link_5"/>
    <child link="right_gripper_link"/>
    <dynamics damping="0.1" friction="0.001"/>
  </joint>

  <joint name="sub_gripper_joint" type="revolute">
    <axis xyz="0 1 0"/>
    <limit effort="1.0" lower="0.1750" upper="1.2741" velocity="1.0"/>
    <origin rpy="0 3.4383 0" xyz="-0.010 0 0.03"/>
    <parent link="link_5"/>
    <child link="left_gripper_link"/>
    <dynamics damping="0.1" friction="0.001"/>
  </joint>

  <!-- ===== Gazebo physics properties ===== -->
  <gazebo reference="gripper_joint">
    <implicitSpringDamper>1</implicitSpringDamper>
  </gazebo>
  <gazebo reference="sub_gripper_joint">
    <implicitSpringDamper>1</implicitSpringDamper>
  </gazebo>
  <gazebo reference="base_link">
    <mu1>1</mu1><mu2>1</mu2>
    <selfCollide>true</selfCollide>
    <material>Gazebo/Turquoise</material>
  </gazebo>
  <gazebo reference="link_1">
    <mu1>1</mu1><mu2>1</mu2>
    <selfCollide>true</selfCollide>
    <material>Gazebo/Green</material>
  </gazebo>
  <gazebo reference="link_2">
    <mu1>1</mu1><mu2>1</mu2>
    <selfCollide>true</selfCollide>
    <material>Gazebo/Orange</material>
  </gazebo>
  <gazebo reference="link_3">
    <mu1>1</mu1><mu2>1</mu2>
    <selfCollide>true</selfCollide>
    <material>Gazebo/Blue</material>
  </gazebo>
  <gazebo reference="link_4">
    <mu1>1</mu1><mu2>1</mu2>
    <selfCollide>true</selfCollide>
    <material>Gazebo/Red</material>
  </gazebo>
  <gazebo reference="link_5">
    <mu1>1</mu1><mu2>1</mu2>
    <selfCollide>true</selfCollide>
    <material>Gazebo/Purple</material>
  </gazebo>
  <gazebo reference="right_gripper_link">
    <selfCollide>true</selfCollide>
    <material>Gazebo/Yellow</material>
    <kp>100000.0</kp><kd>1.0</kd>
    <mu1>500.0</mu1><mu2>200.0</mu2>
    <minDepth>0.002</minDepth><maxVel>0</maxVel>
  </gazebo>
  <gazebo reference="left_gripper_link">
    <selfCollide>true</selfCollide>
    <material>Gazebo/Yellow</material>
    <kp>100000.0</kp><kd>1.0</kd>
    <mu1>500.0</mu1><mu2>200.0</mu2>
    <minDepth>0.002</minDepth><maxVel>0</maxVel>
  </gazebo>

</robot>
